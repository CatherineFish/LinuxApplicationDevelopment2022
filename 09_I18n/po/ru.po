#: src/guessNumber.c:21
#, c-format
msgid "Make a number from 1 to 100\n"
msgstr "Загадайте число от 1 до 100\n"

#: src/guessNumber.c:25
#, c-format
msgid "Is number greater %d?\n"
msgstr "Число больше чем %d?\n"

#: src/guessNumber.c:32
msgid "yes"
msgstr "да"

#: src/guessNumber.c:35
msgid "no"
msgstr "нет"

#: src/guessNumber.c:39
#, c-format
msgid "Wrong answer, 'yes'/'no' are avaliable\n"
msgstr "Неправильный ответ, 'да'/'нет' допустимы\n"

#: src/guessNumber.c:42
#, c-format
msgid "Result! Your number is %d\n"
msgstr "Результат! Ваше число %d\n"
